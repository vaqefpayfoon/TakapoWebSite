                                            <li><a class="dropdown-item text-right"  asp-area="Fa" asp-controller="Home" asp-action="Laboratory" asp-route-name="panther">سیستم اتوماتیک تشخیص مولکولی</a></li>
                                            <li><a class="dropdown-item text-right"  asp-area="Fa" asp-controller="Home" asp-action="Laboratory" asp-route-name="flow">سیستم فلوسایتومتری</a></li>
                                        </ul>
                                    </div>
                                    <div class="col pl-0">
                                        <ul class="list-style-none">
                                            <li><a class="dropdown-item d-flex align-items-center justify-content-between"  asp-area="Fa" asp-controller="Home" asp-action="Laboratory" asp-route-name="thinprep">دستگاه اتوماتیک تهیه لام</a></li>
                                            <li><a class="dropdown-item text-right"  asp-area="Fa" asp-controller="Home" asp-action="Laboratory" asp-route-name="fibro">تست فیبرونکتین جنینی</a></li>
                                            @*<li><a class="dropdown-item text-right"  asp-area="Fa" asp-controller="Home" asp-action="Laboratory" asp-route-name="vaccum">سیستم خونگیری وکیوم</a></li>*@
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
                <div class="d-flex align-items-center">
                    @Html.ActionLink("فارسی", "ChangeLanguage", "Home", new KeyValueModel("lang", "Fa"), new { @class = "btn btn-primary text-secondary d-none d-md-inline-block mr-3" })
                    @Html.ActionLink("English", "ChangeLanguage", "Home", new KeyValueModel("lang", "En"), new { @class = "btn btn-primary text-secondary d-none d-md-inline-block mr-3" })
                </div>
            </div>
        </nav>
    </header>
    <br /><br /><br /><br />
    <main>
        @RenderBody()
    </main>

